algoritmo "semnome"
// Função :
// Autor :
// Data : 21/03/2019
// Seção de Declarações 
var
nasc, atual, idade : inteiro

inicio
escreva ("escreva o ano atual: ")
leia(atual)
escreva ("escreva o ano de nascimento: ")
leia(nasc)
idade <- atual - nasc
escreval("idade atual = ",idade)
idade <- 2050 - nasc
escreval("idade em 2050 = ",idade)


fimalgoritmo
